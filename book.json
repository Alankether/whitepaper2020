{
    "description": "云原生发展白皮书 2020",
    "language": "zh-hans",
    "author": "云原生产业联盟",
    "plugins": [
        "github",
        "codesnippet",
        "splitter",
        "page-toc-button",
        "image-captions",
        "editlink",
        "back-to-top-button",
        "-lunr", "-search", "search-plus",
        "github-buttons@2.1.0",
        "favicon@^0.0.2",
        "tbfed-pagefooter@^0.0.1",
        "3-ba",
        "theme-default",
        "-highlight", "prism", "prism-themes",
        "sitemap-general",
        "lightbox",
        "ga",
        "copy-code-button",
        "alerts"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "github": {
            "url": "https://github.com/CloudNativeIndustryAlliance/whitepaper2020"
        },
        "editlink": {
            "base": "https://github.com/CloudNativeIndustryAlliance/whitepaper2020/blob/master/",
            "label": "编辑本页"
        },
        "image-captions": {
            "caption": "图片 - _CAPTION_"
        },
        "github-buttons": {
            "repo": "CloudNativeIndustryAlliance/whitepaper2020",
            "types": ["star"],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "<a href=http://www.caict.ac.cn/>中国信息通信研究院（CAICT）</a>",
            "modify_label": " Updated at ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "3-ba": {
            "token": "11f7d254cfa4e0ca44b175c66d379ecc"
        },
        "prism": {
            "css": [
            "prism-themes/themes/prism-ghcolors.css"
            ]
        },
        "sitemap-general": {
            "prefix": "https://cloudnativeindustryalliance.github.io/"
        },
        "ga": {
            "token": "UA-93485976-1"
        }
    }
}
